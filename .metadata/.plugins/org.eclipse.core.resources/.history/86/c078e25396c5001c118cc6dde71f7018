#include <stdio.h>
#include <stdlib.h>

#include "utn.h"
#include "operaciones.h"

int main(void) {

	setbuf(stdout,NULL);

	float num;
	int num2;
	int uno,dos;

	uno = utn_getNumeroFlotante(&num, "\nnumero: ", "\nErrorNUm:", 0, 200, 2);
	dos = utn_getNumero(&num2, "\nnumero: ", "\nErrorNUm:", 0, 200, 2);

	printf("Error: %d - numero: %f ",uno,num);
	printf("Error: %d - numero: %d ",dos,num2);

	return 0;


}

